{
    "name": "fecs",
    "displayName": "fecs",
    "description": "fecs extension for vscode",
    "icon": "imges/fecs.jpg",
    "version": "0.10.0",
    "publisher": "Marx",
    "homepage": "https://github.com/MarxJiao/VScode-fecs",
    "bugs": "https://github.com/MarxJiao/VScode-fecs/issues",
    "repository": {
        "type": "git",
        "url": "https://github.com/MarxJiao/VScode-fecs"
    },
    "engines": {
        "vscode": "^1.0.0"
    },
    "enableProposedApi": false,
    "categories": [
        "Linters"
    ],
    "activationEvents": [
        "onCommand:extension.fecs"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.fecs",
                "title": "fecs"
            }
        ],
        "keybindings": [
            {
                "command": "extension.fecs",
                "key": "cmd+shift+r"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "fecs configuration",
            "properties": {
                "fecs.en": {
                    "type": "boolean",
                    "default": false,
                    "description": "Controls if use English in output."
                },
                "fecs.auto": {
                    "type": "boolean",
                    "default": false,
                    "description": "Controls if auto use fecs when saved."
                },
                "fecs.level": {
                    "type": "number",
                    "defaulft": 0,
                    "description": "Fecs check level. Value is 0 1 or 2"
                }
            }
            
        }
    },
    "problemMatchers": [
                {

                    "name": "fecs",

                    "owner": "javascript",

                    "fileLocation": ["relative", "${cwd}"],

                    "pattern": {

                        "regexp": "^fecs\\s+?(.*)\\sâ†’\\sline\\s(\\d+),\\scol\\s(\\d+):\\s(.*)",

                        "line": 2,

                        "column": 3,

                        "severity": 1,

                        "message": 4
                    }
                }
            ],
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "@types/node": "^6.0.40"
    }
}